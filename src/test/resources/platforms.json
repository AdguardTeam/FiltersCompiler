{
    "TEST": {
        "platform": "ext_ublock",
        "path": "test",
        "configuration": {
            "removeRulePatterns": false,
            "ignoreRuleHints": false,
            "adbHeader": "![Adblock Plus 2.0]"
        },
        "defines": {
            "adguard": true,
            "ublock": true
        }
    },
    "TEST_COMMENTS": {
        "platform": "test",
