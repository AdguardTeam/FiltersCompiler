                "\\$\\$|\\$\\@\\$",
                "test-common-1-rule"
            ],
            "ignoreRuleHints": true
        },
        "defines": {
            "adguard": true
        }
    },
    "MAC": {
        "platform": "mac",
        "path": "mac",
        "configuration": {
            "removeRulePatterns": false,
            "ignoreRuleHints": false
        },
        "defines": {
            "adguard": true
        }
    },
    "TEST_HINTS": {
        "platform": "test",
        "path": "hints",
        "configuration": {
            "removeRulePatterns": false,
            "ignoreRuleHints": false
        },
        "defines": {
            "adguard": true
        }
    }
